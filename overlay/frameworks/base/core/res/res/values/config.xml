<!-- These resources are around just to allow their values to be customized for different hardware and product builds. -->
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <bool name="config_animateScreenLights">false</bool>

    <string name="config_datause_iface">rmnet_sdio0</string>
	
    <bool translatable="false" name="config_wifi_p2p_support">true</bool>
	
    <string name="config_wifi_p2p_device_type">10-0050F204-5</string> 

    <string name="config_ethernet_iface_regex">eth\\d</string> 
	
    <bool name="config_sf_limitedAlpha">false</bool> 
	
    <bool name="config_sf_slowBlur">true</bool> 
	
    <bool name="config_automatic_brightness_available">true</bool>
	
    <bool name="config_intrusiveNotificationLed">true</bool>
	
    <bool name="config_autoBrightnessButtonKeyboard">false</bool>

    <!-- The orientation in which case volume keys should be swapped.
         See ROTATION_0, ROTATION_90, ROTATION_180, ROTATION_270 in android.view.Surface
         for more details about values -->
    <integer name="swap_volume_keys_orientation">1</integer>

    <!-- These are all dummies, but need to be present to enable UI control of auto-brightness The actual control and values are all in-kernel -->
